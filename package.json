{
  "name": "vue3-dapp-boot",
  "version": "0.0.6",
  "description": "Start your dapp project of vue3",
  "repository": "https://github.com/justvictory/vue3-dapp-boot.git",
  "license": "MIT",
  "main": "dist/index.common.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist/ && rollup -c",
    "dev:dist": "rollup -w -c rollup.config.js",
    "dev": "vite",
    "build:demo": "vite build",
    "serve:demo": "vite preview",
    "lint": "prettier -c --parser typescript \"{src,__tests__,e2e}/**/*.[jt]s?(x)\"",
    "lint:fix": "yarn run lint --write",
    "test:types": "tsc --build tsconfig.json",
    "test:unit": "jest",
    "test": "yarn run test:types && yarn run test:unit",
    "prepare": "husky install"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@coinbase/wallet-sdk": "^3.1.0",
    "@walletconnect/web3-provider": "^1.7.8",
    "ethers": "^5.6.8"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-inject": "^4.0.4",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/jest": "^27.5.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/compiler-sfc": "^3.2.36",
    "@vue/test-utils": "^2.0.0",
    "@vuedx/typecheck": "^0.7.5",
    "@vuedx/typescript-plugin-vue": "^0.7.5",
    "buffer": "^6.0.3",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "lint-staged": "^12.4.2",
    "pascalcase": "^2.0.0",
    "prettier": "^2.6.2",
    "process": "^0.11.10",
    "rollup": "^2.74.1",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "rollup-plugin-vue": "^6.0.0",
    "ts-jest": "^28.0.3",
    "tslib": "^2.4.0",
    "typescript": "^4.7.2",
    "util": "^0.12.4",
    "vite": "2.9.9",
    "vite-plugin-windicss": "^1.8.4",
    "vue": "3.2.36",
    "windicss": "^3.5.4"
  },
  "lint-staged": {
    "**/*.{js,ts,json,scss,css,vue}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "vue3",
    "dapp",
    "web3",
    "ethereum",
    "ether",
    "hook"
  ]
}
